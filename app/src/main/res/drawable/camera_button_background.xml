<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item>
    <shape android:shape="oval"
        xmlns:android="http://schemas.android.com/apk/res/android">
        <stroke
            android:width="3dp"
            android:color="@color/white"/>
    </shape>
</item>

    <item
        android:top="@dimen/vertical_margin"
        android:bottom="@dimen/vertical_margin"
        android:right="@dimen/horizontal_margin"
        android:left="@dimen/horizontal_margin">
        <shape android:shape="oval"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <solid
                android:color="@color/white"/>
            <size
                android:height="65dp"
                android:width="65dp"/>
        </shape>
    </item>

</layer-list>